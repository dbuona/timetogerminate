	Journal = {Ecology},
	Keywords = {Bombus spp, bumble bee, climate change, Corydalis ambigua, flowering phenology, hibernation, Hokkaido, Japan, phenological mismatch, pollinator activity, snowmelt time, spring ephemerals},
	Number = {10},
	Pages = {2311-2320},
	Title = {Early onset of spring increases the phenological mismatch between plants and pollinators},
