	Journal = {Global Ecology and Biogeography},
	Keywords = {Connectivity, countryside biogeography, edge effects, extinction proneness, habitat loss, habitat fragmentat, keystone, species, landscape heterogeneity, matrix, threatening processes},
	Number = {3},
	Pages = {265-280},
	Title = {Landscape modification and habitat fragmentation: a synthesis},
