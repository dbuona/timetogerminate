@article{Bossuyt:2001aa,
	Annote = {doi: 10.1034/j.1600-0587.2001.240213.x},
	Author = {Bossuyt, Beatrijs and Hermy, Martin},
	Booktitle = {Ecography},
	Da = {2001/04/01},
	Date = {2001/04/01},
	Date-Added = {2020-08-11 14:42:14 -0400},
	Date-Modified = {2020-08-11 14:42:14 -0400},
