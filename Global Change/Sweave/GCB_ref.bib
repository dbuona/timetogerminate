@article{Sabine_2004,
	Abstract = {Using inorganic carbon measurements from an international survey effort in the 1990s and a tracer-based separation technique, we estimate a global oceanic anthropogenic carbon dioxide (CO2) sink for the period from 1800 to 1994 of 118 {\textpm} 19 petagrams of carbon. The oceanic sink accounts for \~{}48\% of the total fossil-fuel and cement-manufacturing emissions, implying that the terrestrial biosphere was a net source of CO2 to the atmosphere of about 39 {\textpm} 28 petagrams of carbon for this period. The current fraction of total anthropogenic CO2 emissions stored in the ocean appears to be about one-third of the long-term potential.},
	Author = {Sabine, Christopher L. and Feely, Richard A. and Gruber, Nicolas and Key, Robert M. and Lee, Kitack and Bullister, John L. and Wanninkhof, Rik and Wong, C. S. and Wallace, Douglas W. R. and Tilbrook, Bronte and Millero, Frank J. and Peng, Tsung-Hung and Kozyr, Alexander and Ono, Tsueno and Rios, Aida F.},
