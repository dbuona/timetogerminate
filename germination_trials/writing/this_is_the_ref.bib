@article{BIERZYCHUDEK:1982aa,
	Annote = {doi: 10.1111/j.1469-8137.1982.tb03285.x},
	Author = {BIERZYCHUDEK, PAULETTE},
	Booktitle = {New Phytologist},
	Da = {1982/04/01},
	Date = {1982/04/01},
	Date-Added = {2020-08-11 13:28:09 -0400},
