	Journal = {New Phytologist},
	Keywords = {elevated CO2, meta-analysis, seed mass, RGR, photosynthesis, growth, functional groups, competition},
	Number = {2},
	Pages = {175-198},
	Title = {Plant growth and competition at elevated CO2: on winners, losers and functional groups},
	Url = {https://nph.onlinelibrary.wiley.com/doi/abs/10.1046/j.1469-8137.2003.00680.x},
	Volume = {157},
