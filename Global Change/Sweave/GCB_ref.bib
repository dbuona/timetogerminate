	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences},
	Number = {39},
	Pages = {14165--14169},
	Publisher = {National Academy of Sciences},
	Title = {Natural selection on thermal performance in a novel thermal environment},
