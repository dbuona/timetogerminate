	Issn = {0378-1127},
	Journal = {Forest Ecology and Management},
	Keywords = {Adaptive management, Collaborative Forest Landscape Restoration Program (CFLRP), Colorado Front Range, Ecological restoration, Fuel hazard reduction, , Douglas-fir,},
	Pages = {191 - 204},
	Title = {Collaborative restoration effects on forest structure in ponderosa pine-dominated forests of Colorado},
	Url = {http://www.sciencedirect.com/science/article/pii/S0378112718303335},
	Volume = {424},
