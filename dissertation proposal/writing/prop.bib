@article{Gioria2018,
	Annote = {10.1093/jpe/rtw105},
	Author = {Gioria, Margherita and Py{\v s}ek, Petr and Osborne, Bruce A},
	Date = {2018/01/19},
	Date-Added = {2018-08-21 13:20:50 -0400},
